{
  "target": {
    "path": "deployment/k8s-local-remote-demo-backend/container/k8s-local-remote-demo-backend",
    "namespace": "demo"
  },
  "feature": {
    "env": {
      "include": "DB_URL;SQS_QUEUE_URL;AWS_WEB_IDENTITY_TOKEN_FILE;AWS_ROLE_ARN;AWS_REGION;AWS_DEFAULT_REGION;AWS_STS_REGIONAL_ENDPOINTS",
      "override": {
        "VERSION": "mirrord"
      }
    },
    "fs": true,
    "network": {
      "incoming": true,
      "outgoing": true
    }
  },
  "agent": {
    "namespace": "demo"
  }
}